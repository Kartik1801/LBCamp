<%- layout("/layouts/boilerplate") %> 
<h2>Add a new Campground</h2>
  <form action="/campgrounds" method="POST">
      <div>
          <label for="title">Title</label>
          <input type = "text" id = "title" name = "campgrounds[title]">
      </div>
      <div>
          <label for="">Location</label>
          <input type = "text" id = "location" name = "campgrounds[location]">
      </div>
      <button>Add a campground</button>
  </form>